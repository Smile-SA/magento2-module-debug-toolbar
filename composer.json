{
    "name": "smile/module-debug-toolbar",
    "type": "magento2-module",
    "description": "Smile Debug Toolbar",
    "homepage": "https://github.com/Smile-SA/magento2-module-debug-toolbar",
    "keywords": ["magento2", "debug", "toolbar"],
    "authors": [
        {
            "name": "Smile Technical Office",
            "email": "dirtech@smile.fr"
        }
    ],
    "license": "EPL-2.0",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.3 || ^8.1",
        "magento/framework": ">=103.0.0",
        "magento/module-config": ">=101.2.0"
    },
    "require-dev": {
        "magento/magento-coding-standard": "^6.0",
        "phpmd/phpmd": "^2.10",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Smile\\DebugToolbar\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}
