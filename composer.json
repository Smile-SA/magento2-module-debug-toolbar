{
    "name": "smile/module-debug-toolbar",
    "type": "magento2-module",
    "description": "Smile Debug Toolbar",
    "homepage": "https://github.com/Smile-SA/magento2-module-debug-toolbar",
    "keywords": ["magento2", "debug", "toolbar"],
    "authors": [
        {
            "name": "Smile Technical Office",
            "email": "dirtech@smile.fr"
        }
    ],
    "license": "EPL-2.0",
    "require": {
        "php": ">=7.3.0",
        "magento/framework": ">=103.0.0",
        "magento/module-config": ">=101.2.0"
    },
    "require-dev": {
        "magento/magento-coding-standard": "^6.0",
        "phpmd/phpmd": "^2.10"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Smile\\DebugToolbar\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/phpcs --config-set installed_paths ../../magento/magento-coding-standard"
        ]
    }
}
